{
  "name": "sepilot-wiki",
  "version": "0.2.0",
  "description": "AI 에이전트 기반 GitHub Wiki 자동화 시스템 - Keycloak OAuth 지원",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:vite": "vite",
    "build:wiki": "node scripts/builders/build-wiki-data.js",
    "build:search": "node scripts/builders/build-search-index.js",
    "sync:issues": "node scripts/issue/sync-issues.js",
    "collect": "node scripts/collectors/collect-dynamic-pages.js",
    "build:404": "node scripts/builders/build-404.js",
    "build": "npm run build:wiki && npm run build:search && next build",
    "build:static": "BUILD_MODE=static AUTH_MODE=public npm run build:wiki && npm run build:search && next build",
    "build:pages": "npm run build:static",
    "build:vite": "npm run build:wiki && npm run build:search && vite build && npm run build:404",
    "start": "next start",
    "preview": "vite preview",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/core": "^0.37.0",
    "@cp949/react-wordcloud": "^1.0.1",
    "@tanstack/react-query": "^5.0.0",
    "@types/dompurify": "^3.0.5",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "dompurify": "^3.3.0",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.555.0",
    "mermaid": "^11.12.2",
    "next": "^15.0.0",
    "next-auth": "^5.0.0-beta.25",
    "plotly.js": "^3.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^7.10.1",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/plotly.js": "^3.0.8",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-plotly.js": "^2.6.3",
    "@types/react-syntax-highlighter": "^15.5.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.1",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "SEE LICENSE IN LICENSE"
}
