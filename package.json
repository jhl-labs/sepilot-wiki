{
  "name": "sepilot-wiki",
  "version": "0.4.0",
  "description": "AI 에이전트 기반 GitHub Wiki 자동화 시스템 - Keycloak OAuth 지원",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:vite": "vite",
    "build:wiki": "node scripts/builders/build-wiki-data.js",
    "build:search": "node scripts/builders/build-search-index.js",
    "sync:issues": "node scripts/issue/sync-issues.js",
    "collect": "node scripts/collectors/collect-dynamic-pages.js",
    "build:404": "node scripts/builders/build-404.js",
    "build": "npm run build:wiki && npm run build:search && next build",
    "build:static": "mv app/api app/_api_excluded && export BUILD_MODE=static AUTH_MODE=public && npm run build:wiki && npm run build:search && next build; EXIT=$?; mv app/_api_excluded app/api; exit $EXIT",
    "build:pages": "npm run build:static",
    "build:vite": "npm run build:wiki && npm run build:search && vite build && npm run build:404",
    "start": "next start",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/core": "^0.41.1",
    "@cp949/react-wordcloud": "^1.0.1",
    "@monaco-editor/react": "^4.7.0",
    "@octokit/rest": "^22.0.1",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-virtual": "^3.13.13",
    "@types/dompurify": "^3.0.5",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "dompurify": "^3.3.0",
    "fuse.js": "^7.1.0",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.563.0",
    "mermaid": "^11.12.2",
    "monaco-editor": "^0.55.1",
    "next": "^16.1.6",
    "next-auth": "^5.0.0-beta.25",
    "node-cron": "^4.2.1",
    "plotly.js": "^3.3.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^7.13.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.0",
    "@types/node-cron": "^3.0.11",
    "@types/plotly.js": "^3.0.8",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/react-plotly.js": "^2.6.4",
    "@types/react-syntax-highlighter": "^15.5.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "husky": "^8.0.0",
    "jsdom": "^28.0.0",
    "monaco-editor": "^0.55.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "lodash-es": "^4.17.23"
  },
  "license": "SEE LICENSE IN LICENSE"
}
